            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register",
                "operationId": "af9672879ca2c8ba3cf2355530539d31",
                "requestBody": {
                    "description": "Register (Type 1:Customer,2:Driver,3:Partner)",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "0964541340"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "example": "0964541340"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "123456"
                                    },
                                    "type": {
                                        "type": "integer",
                                        "example": "1"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Register successfully"
                    },
                    "422": {
                        "description": "Validation error"
                    }
                }
            }
        },
